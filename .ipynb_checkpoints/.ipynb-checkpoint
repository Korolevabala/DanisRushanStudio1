    "#Добавляем нужные нам столбцы\n",
    "data['long'] = 0\n",
    "data['short'] = 0\n",
    "data['long exit'] = 0\n",
    "data['short exit'] = 0\n",
    "data['position'] = 0\n",
    "data[''] = 0\n",
    "data[''] = data[''].astype('float64') #Этот стобец требуется нам в типе float\n",
    "data['Сделка'] = np.nan\n",
